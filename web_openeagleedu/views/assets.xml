<?xml version="1.0" encoding="UTF-8"?>
<eagle>
    <template id="web_openeagleedu_assets_frontend" name="Theme OpenEducat Frontend Assets"
              inherit_id="website.assets_frontend">
        <xpath expr="." position="inside">
            <link href="/web_openeagleedu/static/src/scss/homepage.scss"
                  type="text/scss" rel="stylesheet"/>
        </xpath>
    </template>

    <template id="snippet_id" inherit_id="website.snippets">
        <xpath expr="//div[@id='snippet_structure']" position="before">
            <div class="o_panel_body">
                <!--________________________ Snippets____________________ -->
                        <t t-snippet="web_openeagleedu.slider"
                           t-thumbnail="web_openeagleedu/static/src/img/snippets_icons/op_educat.png"/>
                        <t t-snippet="web_openeagleedu.about-us"
                           t-thumbnail="web_openeagleedu/static/src/img/snippets_icons/op_educat.png"/>
                        <t t-snippet="web_openeagleedu.ourcourse"
                           t-thumbnail="web_openeagleedu/static/src/img/snippets_icons/op_educat.png"/>
                        <t t-snippet="web_openeagleedu.achievement"
                           t-thumbnail="web_openeagleedu/static/src/img/snippets_icons/op_educat.png"/>
                        <t t-snippet="web_openeagleedu.teacher"
                           t-thumbnail="web_openeagleedu/static/src/img/snippets_icons/op_educat.png"/>
                        <t t-snippet="web_openeagleedu.event"
                           t-thumbnail="web_openeagleedu/static/src/img/snippets_icons/op_educat.png"/>
                        <t t-snippet="web_openeagleedu.newsfeed"
                           t-thumbnail="web_openeagleedu/static/src/img/snippets_icons/op_educat.png"/>
                        <t t-snippet="web_openeagleedu.footer"
                           t-thumbnail="web_openeagleedu/static/src/img/snippets_icons/op_educat.png"/>
            </div>
        </xpath>
    </template>
</eagle>
